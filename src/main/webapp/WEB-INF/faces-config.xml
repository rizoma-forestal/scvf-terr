<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>

    <navigation-rule>
        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <managed-bean>
        <managed-bean-name>mbEspRegion</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionterritorial.managedBeans.MbEspRegion</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>mbRegion</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionterritorial.managedBeans.MbRegion</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbProvincia</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionterritorial.managedBeans.MbProvincia</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>mbDepartamento</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionterritorial.managedBeans.MbDepartamento</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>mbMunicipio</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionterritorial.managedBeans.MbMunicipio</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
   
    <managed-bean>
        <managed-bean-name>mbCentroPoblado</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionterritorial.managedBeans.MbCentroPoblado</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>mbCentroPobladoTipo</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionterritorial.managedBeans.MbCentroPobladoTipo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
</faces-config>
